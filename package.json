{
  "name": "rlscrapper",
  "version": "0.0.0",
  "description": "rocket league spanish database pwa",
  "main": "index.js",
  "private": true,
  "scripts": {
    "install": "yarn",
    "start": "bankai start index.js",
    "gen": "choo g",
    "build": "bankai build index.js && cp CNAME dist/",
    "deploy": "gh-pages -d dist -s \"**/*+(CNAME|.html|.js|.css|.json|.png|.svg)\"",
    "lint": "standard --verbose | snazzy",
    "deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons",
    "test": "npm run lint && npm run deps",
    "inspect": "bankai inspect index.js",
    "clean": "rm -r dist/",
    "crawl": "node crawler.js",
    "commit_db": "git add assets/data/db.json && git commit -m \"chore: data sync\" && git push origin master",
    "sync": "npm run commit_db && npm run build && npm run deploy"
  },
  "keywords": [
    "rocket",
    "league",
    "mmr",
    "ranking",
    "website",
    "salve"
  ],
  "author": "neko250",
  "license": "MIT",
  "dependencies": {
    "choo": "^6.13.1",
    "choo-service-worker": "^2.4.0",
    "sheetify": "^7.3.3",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "bankai": "^9.15.0",
    "choo-cli": "^2.1.0",
    "choo-devtools": "^2.5.1",
    "crawler": "^1.2.0",
    "dependency-check": "^3.3.0",
    "gh-pages": "^2.0.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  }
}
